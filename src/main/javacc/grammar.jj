BUTree<SPackageDecl> PackageDecl():
{
	BUTree<SNodeList> annotations = null;
	BUTree<SQualifiedName> name;
}
{
  { run(); }

  annotations = Annotations() "package" name = QualifiedName() ";"

  { return dress(SPackageDecl.make(annotations, name)); }
}

BUTree<SNodeList> ImportDecls():
{
	BUTree<SNodeList> imports = emptyList();
	BUTree<SImportDecl> importDecl = null;
}
{
  ( importDecl = ImportDecl() { imports = append(imports, importDecl); } )*
  { return imports; }
}

BUTree<SImportDecl> ImportDecl():
{
	BUTree<SQualifiedName> name;
	boolean isStatic = false;
	boolean isAsterisk = false;
}
{
  { run(); }
  "import" [ "static" { isStatic = true; } ] name = QualifiedName() [ "." "*" { isAsterisk = true; } ] ";"
  { return dress(SImportDecl.make(name, isStatic, isAsterisk)); }
}

BUTree<SNodeList> TypeDecls():
{
	BUTree<SNodeList> types = emptyList();
	BUTree<? extends STypeDecl> typeDecl = null;
}
{
  ( typeDecl = TypeDecl() { types = append(types, typeDecl); } )*
  { return types; }
}

/*
 * Modifiers. We match all modifiers in a single rule to reduce the chances of
 * syntax errors for simple modifier mistakes. It will also enable us to give
 * better error messages.
 */

BUTree<SNodeList> Modifiers():
{
	BUTree<SNodeList> modifiers = emptyList();
	BUTree<? extends SAnnotationExpr> ann;
}
{
  (
    LOOKAHEAD(2)
    (
      "public" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Public)); }
    |
      "protected" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Protected)); }
    |
      "private" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Private)); }
    |
      "abstract" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Abstract)); }
    |
      "default" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Default)); }
    |
      "static" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Static)); }
    |
      "final" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Final)); }
    |
      "transient" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Transient)); }
    |
      "volatile" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Volatile)); }
    |
      "synchronized" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Synchronized)); }
    |
      "native" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Native)); }
    |
      "strictfp" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.StrictFP)); }
    |
      ann = Annotation() { modifiers = append(modifiers, ann); }
    )
  )*

  { return modifiers; }
}

BUTree<SNodeList> ModifiersNoDefault():
{
	BUTree<SNodeList> modifiers = emptyList();
	BUTree<? extends SAnnotationExpr> ann;
}
{
  (
    LOOKAHEAD(2)
    (
      "public" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Public)); }
    |
      "protected" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Protected)); }
    |
      "private" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Private)); }
    |
      "abstract" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Abstract)); }
    |
      "static" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Static)); }
    |
      "final" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Final)); }
    |
      "transient" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Transient)); }
    |
      "volatile" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Volatile)); }
    |
      "synchronized" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Synchronized)); }
    |
      "native" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.Native)); }
    |
      "strictfp" { modifiers = append(modifiers, SModifier.make(ModifierKeyword.StrictFP)); }
    |
      ann = Annotation() { modifiers = append(modifiers, ann); }
    )
  )*

  { return modifiers; }
}

/*
 * Decl syntax follows.
 */
BUTree<? extends STypeDecl> TypeDecl():
{
	BUTree<SNodeList> modifiers;
	BUTree<? extends STypeDecl> ret;
}
{
  { run(); }

  (
    ";" { ret = dress(SEmptyTypeDecl.make()); }
  |
    modifiers = Modifiers()
    (
      ret = ClassOrInterfaceDecl(modifiers)
    |
      ret = EnumDecl(modifiers)
    |
      ret = AnnotationTypeDecl(modifiers)
    )
  )

  { return ret; }
}


BUTree<? extends STypeDecl> ClassOrInterfaceDecl(BUTree<SNodeList> modifiers):
{
	TypeKind typeKind;
	BUTree<SName> name;
	BUTree<SNodeList> typeParams = null;
	BUTree<SQualifiedType> superClassType = null;
	BUTree<SNodeList> extendsClause = null;
	BUTree<SNodeList> implementsClause = null;
	BUTree<SNodeList> members;
}
{
  // Run is started before modifiers

  (
    "class" { typeKind = TypeKind.Class; }
    name = Name()
    [ typeParams = TypeParameters() ]
    [ "extends" superClassType = AnnotatedQualifiedType() ]
    [ implementsClause = ImplementsList(typeKind) ]
  |
    "interface" { typeKind = TypeKind.Interface; }
    name = Name()
    [ typeParams = TypeParameters() ]
    [ extendsClause = ExtendsList() ]
  )
  members = ClassOrInterfaceBody(typeKind)

  {
    if (typeKind == TypeKind.Interface)
      return dress(SInterfaceDecl.make(modifiers, name, ensureNotNull(typeParams), ensureNotNull(extendsClause), members));
    else {
      return dress(SClassDecl.make(modifiers, name, ensureNotNull(typeParams), optionOf(superClassType), ensureNotNull(implementsClause), members));
    }
  }
}

BUTree<SNodeList> ExtendsList():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<SQualifiedType> cit;
	BUTree<SNodeList> annotations = null;
}
{
  "extends"
  (
    cit = AnnotatedQualifiedType() { ret = append(ret, cit); }
    ( "," cit = AnnotatedQualifiedType() { ret = append(ret, cit); } )*
  |
    LOOKAHEAD({quotesMode}) ret = NodeListVar()
  )
  { return ret; }
}

BUTree<SNodeList> ImplementsList(TypeKind typeKind):
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<SQualifiedType> cit;
	BUTree<SNodeList> annotations = null;
}
{
  "implements"
  (
    cit = AnnotatedQualifiedType() { ret = append(ret, cit); }
    ( "," cit = AnnotatedQualifiedType() { ret = append(ret, cit); } )*
    {
      if (typeKind == TypeKind.Interface)
        throwParseException(token, "An interface cannot implement other interfaces");
    }
  |
    LOOKAHEAD({quotesMode}) ret = NodeListVar()
  )
  { return ret; }
}

