/**********************************************************************************************************************
 * Expressions
 **********************************************************************************************************************/

BUTree<? extends SExpr> Expression():
// FIXME
// This expansion has been written this way instead of :
//   Assignment() | ConditionalExpression()
// for performance reasons.
// However, it is a weakening of the grammar for it allows the LHS of
// assignments to be any conditional Expr whereas it can only be
// a primary Expr.  Consider adding a semantic predicate to work
// around this.
{
	BUTree<? extends SExpr> ret;
	AssignOp op;
	BUTree<? extends SExpr> value;
	BUTree<SNodeList> params;
}
{
  (
    LOOKAHEAD( Name() "->" )
    { run(); } ret = Name() "->" ret = LambdaBody(singletonList(makeFormalParameter((BUTree<SName>) ret)), false)
  |
    LOOKAHEAD( "(" ")" "->" )
    { run(); } "(" ")" "->" ret = LambdaBody(emptyList(), true)
  |
    LOOKAHEAD( "(" Name() ")" "->" )
    { run(); } "(" ret = Name() ")" "->" ret = LambdaBody(singletonList(makeFormalParameter((BUTree<SName>) ret)), true)
  |
    LOOKAHEAD( "(" Name() "," )
    { run(); } "(" params = InferredFormalParameterList() ")" "->" ret = LambdaBody(params, true)
  |
    ret = ConditionalExpression()
    [
      LOOKAHEAD(2)
      { lateRun(); } op = AssignmentOperator() value = Expression()
      { ret = dress(SAssignExpr.make(ret, op, value)); }
    ]
  )
  { return ret; }
}

BUTree<SLambdaExpr> LambdaBody(BUTree<SNodeList> parameters, boolean parenthesis):
{
	BUTree<SBlockStmt> block;
	BUTree<? extends SExpr> expr;
	BUTree<SLambdaExpr> ret;
}
{
  // Run was started before parameters
  (
    expr = Expression() { ret = dress(SLambdaExpr.make(parameters, parenthesis, left(expr))); }
  |
    block = Block() { ret = dress(SLambdaExpr.make(parameters, parenthesis, right(block))); }
  )
  { return ret; }
}

BUTree<SNodeList> InferredFormalParameterList():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<SFormalParameter> param;
}
{
  param = InferredFormalParameter() { ret = append(ret, param); }
  ( "," param = InferredFormalParameter() { ret = append(ret, param); } )*
  { return ret; }
}

BUTree<SFormalParameter> InferredFormalParameter():
{
	BUTree<SName> name;
}
{
  name = Name()
  { return makeFormalParameter(name); }
}

JAVACODE
BUTree<SFormalParameter> makeFormalParameter(BUTree<SName> name) {
  return SFormalParameter.make(emptyList(), SUnknownType.make(), false,
    SVariableDeclaratorId.make(name, emptyList())
  );
}

AssignOp AssignmentOperator():
{
	AssignOp ret;
}
{
  ( "="    { ret = AssignOp.Normal; }
  | "*="   { ret = AssignOp.Times; }
  | "/="   { ret = AssignOp.Divide; }
  | "%="   { ret = AssignOp.Remainder; }
  | "+="   { ret = AssignOp.Plus; }
  | "-="   { ret = AssignOp.Minus; }
  | "<<="  { ret = AssignOp.LeftShift; }
  | ">>="  { ret = AssignOp.RightSignedShift; }
  | ">>>=" { ret = AssignOp.RightUnsignedShift; }
  | "&="   { ret = AssignOp.And; }
  | "^="   { ret = AssignOp.XOr; }
  | "|="   { ret = AssignOp.Or; } )
  { return ret; }
}

BUTree<? extends SExpr> ConditionalExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> left;
	BUTree<? extends SExpr> right;
}
{
  ret = ConditionalOrExpression()
  [
    LOOKAHEAD(2)
    { lateRun(); } "?" left = Expression() ":" right = ConditionalExpression()
    { ret = dress(SConditionalExpr.make(ret, left, right)); }
  ]
  { return ret; }
}

BUTree<? extends SExpr> ConditionalOrExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
}
{
  ret = ConditionalAndExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); } "||" right = ConditionalAndExpression()
    { ret = dress(SBinaryExpr.make(ret, BinaryOp.Or, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> ConditionalAndExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
}
{
  ret = InclusiveOrExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); } "&&" right = InclusiveOrExpression()
    { ret = dress(SBinaryExpr.make(ret, BinaryOp.And, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> InclusiveOrExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
}
{
  ret = ExclusiveOrExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); } "|" right = ExclusiveOrExpression()
    { ret = dress(SBinaryExpr.make(ret, BinaryOp.BinOr, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> ExclusiveOrExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
}
{
  ret = AndExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); } "^" right = AndExpression()
    { ret = dress(SBinaryExpr.make(ret, BinaryOp.XOr, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> AndExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
}
{
  ret = EqualityExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); } "&" right = EqualityExpression()
    { ret = dress(SBinaryExpr.make(ret, BinaryOp.BinAnd, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> EqualityExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
	BinaryOp op;
}
{
  ret = InstanceOfExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); }
    ( "==" { op = BinaryOp.Equal; } |
      "!=" { op = BinaryOp.NotEqual; } )
    right = InstanceOfExpression()
    { ret = dress(SBinaryExpr.make(ret, op, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> InstanceOfExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<SNodeList> annotations;
	BUTree<? extends SType> type;
}
{
  ret = RelationalExpression()
  [
    LOOKAHEAD(2)
    { lateRun(); } "instanceof"
    { run(); } annotations = Annotations() type = Type(annotations)
    { ret = dress(SInstanceOfExpr.make(ret, type)); }
  ]
  { return ret; }
}

BUTree<? extends SExpr> RelationalExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
	BinaryOp op;
}
{
  ret = ShiftExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); }
  	( "<"  { op = BinaryOp.Less; } |
  	  ">"  { op = BinaryOp.Greater; } |
  	  "<=" { op = BinaryOp.LessOrEqual; } |
  	  ">=" { op = BinaryOp.GreaterOrEqual; } )
  	right = ShiftExpression()
  	{ ret = dress(SBinaryExpr.make(ret, op, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> ShiftExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
	BinaryOp op;
}
{
  ret = AdditiveExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); }
  	( "<<"             { op = BinaryOp.LeftShift; } |
  	  RSIGNEDSHIFT()   { op = BinaryOp.RightSignedShift; } |
  	  RUNSIGNEDSHIFT() { op = BinaryOp.RightUnsignedShift; } )
  	right = AdditiveExpression()
  	{ ret = dress(SBinaryExpr.make(ret, op, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> AdditiveExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
	BinaryOp op;
}
{
  ret = MultiplicativeExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); }
  	( "+" { op = BinaryOp.Plus; } |
  	  "-" { op = BinaryOp.Minus; } )
  	right = MultiplicativeExpression()
  	{ ret = dress(SBinaryExpr.make(ret, op, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> MultiplicativeExpression():
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SExpr> right;
	BinaryOp op;
}
{
  ret = UnaryExpression()
  (
    LOOKAHEAD(2)
    { lateRun(); }
  	( "*" { op = BinaryOp.Times; } |
  	  "/" { op = BinaryOp.Divide; } |
  	  "%" { op = BinaryOp.Remainder; } )
  	right = UnaryExpression()
  	{ ret = dress(SBinaryExpr.make(ret, op, right)); }
  )*
  { return ret; }
}

BUTree<? extends SExpr> UnaryExpression():
{
	BUTree<? extends SExpr> ret;
	UnaryOp op;
}
{
  (
    ret = PrefixExpression()
  |
    { run(); }
    ( "+" { op = UnaryOp.Positive; } |
      "-" { op = UnaryOp.Negative; } )
    ret = UnaryExpression()
    { ret = dress(SUnaryExpr.make(op, ret)); }
  |
    ret = UnaryExpressionNotPlusMinus()
  )
  { return ret; }
}

BUTree<? extends SExpr> PrefixExpression():
{
	UnaryOp op;
	BUTree<? extends SExpr> ret;
}
{
  { run(); }
  ( "++" { op = UnaryOp.PreIncrement; } |
    "--" { op = UnaryOp.PreDecrement; } )
  ret = UnaryExpression()
  { return dress(SUnaryExpr.make(op, ret)); }
}

BUTree<? extends SExpr> UnaryExpressionNotPlusMinus():
{
	BUTree<? extends SExpr> ret;
	UnaryOp op;
}
{
  (
    { run(); }
	( "~" { op = UnaryOp.Inverse; } |
	  "!" { op = UnaryOp.Not; } )
	ret = UnaryExpression()
	{ ret = dress(SUnaryExpr.make(op, ret)); }
  |
    LOOKAHEAD(CastExpression())
//    LOOKAHEAD( "(", {isCast(false)} )
    ret = CastExpression()
  |
    ret = PostfixExpression()
  )
  { return ret; }
}

BUTree<? extends SExpr> PostfixExpression():
{
	BUTree<? extends SExpr> ret;
	UnaryOp op;
}
{
  ret = PrimaryExpression()
  [
  	LOOKAHEAD(2)
    { lateRun(); }
    ( "++" { op = UnaryOp.PostIncrement; } |
      "--" { op = UnaryOp.PostDecrement; } )
    { ret = dress(SUnaryExpr.make(op, ret)); }
  ]
  { return ret; }
}

BUTree<? extends SExpr> CastExpression():
{
	BUTree<SNodeList> annotations = null;
	BUTree<? extends SType> primitiveType;
	BUTree<? extends SType> type;
	BUTree<SNodeList> arrayDims;
	BUTree<? extends SExpr> ret;
}
{
  { run(); }
  "("
  { run(); } annotations = Annotations()
  (
    primitiveType = PrimitiveType(annotations)
    (
      ")"
      ret = UnaryExpression()
      { ret = dress(SCastExpr.make(primitiveType, ret)); }
    |
      { lateRun(); }
      arrayDims = ArrayDimsMandatory()
      { type = dress(SArrayType.make(primitiveType, arrayDims)); }
      type = ReferenceCastTypeRest(type)
      ")"
      ret = UnaryExpressionNotPlusMinus()
      { ret = dress(SCastExpr.make(type, ret)); }
    )
  |
    type = QualifiedType(annotations)
    [
      LOOKAHEAD( Annotations() "[" )
      { lateRun(); }
      arrayDims = ArrayDimsMandatory()
      { type = dress(SArrayType.make(type, arrayDims)); }
    ]
    type = ReferenceCastTypeRest(type)
    ")"
    ret = UnaryExpressionNotPlusMinus()
    { ret = dress(SCastExpr.make(type, ret)); }
  )
  { return ret; }
}

BUTree<? extends SType> ReferenceCastTypeRest(BUTree<? extends SType> type):
{
	BUTree<SNodeList> types = emptyList();
	BUTree<SNodeList> annotations = null;
}
{
  [
    LOOKAHEAD("&")
    { types = append(types, type); }
    { lateRun(); }
    (
      "&"
      { run(); } annotations = Annotations() type = ReferenceType(annotations)
      { types = append(types, type); }
    )+
    { type = dress(SIntersectionType.make(types)); }
  ]
  { return type; }
}

BUTree<? extends SExpr> Literal():
{
	BUTree<? extends SExpr> ret;
}
{
  { run(); }
  (
    <INTEGER_LITERAL>   { ret = SLiteralExpr.make(Integer.class, token.image); }
  |
    <LONG_LITERAL>      { ret = SLiteralExpr.make(Long.class, token.image); }
  |
    <FLOAT_LITERAL>     { ret = SLiteralExpr.make(Float.class, token.image); }
  |
    <DOUBLE_LITERAL>    { ret = SLiteralExpr.make(Double.class, token.image); }
  |
    <CHARACTER_LITERAL> { ret = SLiteralExpr.make(Character.class, token.image); }
  |
    <STRING_LITERAL>    { ret = SLiteralExpr.make(String.class, token.image); }
  |
    "true"  { ret = SLiteralExpr.make(Boolean.class, token.image); }
  |
    "false" { ret = SLiteralExpr.make(Boolean.class, token.image); }
  |
    "null"  { ret = SLiteralExpr.make(Void.class, token.image); }
  )
  { return dress(ret); }
}

BUTree<? extends SExpr> PrimaryExpression():
{
	BUTree<? extends SExpr> ret;
}
{
  ret = PrimaryPrefix()
  (
    LOOKAHEAD(2)
    { lateRun(); } ret = PrimarySuffix(ret)
  )*
  { return ret; }
}

BUTree<? extends SExpr> PrimaryExpressionWithoutSuperSuffix():
{
	BUTree<? extends SExpr> ret;
}
{
  ret = PrimaryPrefix()
  (
    LOOKAHEAD( PrimarySuffixWithoutSuper() )
    { lateRun(); } ret = PrimarySuffixWithoutSuper(ret)
  )*
  { return ret; }
}

BUTree<? extends SExpr> PrimaryPrefix():
{
	BUTree<? extends SExpr> ret = null;
	BUTree<SNodeList> typeArgs = null;
	BUTree<SNodeList> params;
	BUTree<? extends SType> type;
}
{
  (
    ret = Literal()
  |
    { run(); } "this" { ret = dress(SThisExpr.make(none())); }
  |
    { run(); } "super" { ret = dress(SSuperExpr.make(none())); }
	(
	  { lateRun(); }
	  "."
	  (
        LOOKAHEAD( [ TypeArguments() ] Name() "(" )
        ret = MethodInvocation(ret)
	  |
	    ret = FieldAccess(ret)
	  )
	|
	  { lateRun(); }
	  ret = MethodReferenceSuffix(ret)
	)
  |
    ret = AllocationExpression(null)
  |
    LOOKAHEAD( ResultType() "." "class" )
    { run(); } type = ResultType() "." "class" { ret = dress(SClassExpr.make(type)); }
  |
    LOOKAHEAD (ResultType() "::" )
    { run(); }
    type = ResultType() { ret = STypeExpr.make(type); }
    ret = MethodReferenceSuffix(ret)
  |
    LOOKAHEAD( [ TypeArguments() ] Name() "(" )
    { run(); }
    ret = MethodInvocation(null)
  |
    ret = Name()
    [
      { lateRun(); }
      "->" ret = LambdaBody(singletonList(makeFormalParameter((BUTree<SName>) ret)), false)
    ]
  |
    { run(); }
    "("
    (
      ")" "->" ret = LambdaBody(emptyList(), true)
    |
      LOOKAHEAD( Name() ")" "->" )
      ret = Name() ")" "->" ret = LambdaBody(singletonList(makeFormalParameter((BUTree<SName>) ret)), true)
    |
      LOOKAHEAD( Name() "," )
      params = InferredFormalParameterList() ")" "->" ret = LambdaBody(params, true)
    |
      LOOKAHEAD({ isLambda(true) })
      params = FormalParameterList() ")" "->" ret = LambdaBody(params, true)
    |
      ret = Expression() ")"
      { ret = dress(SParenthesizedExpr.make(ret)); }
    )
  )
  { return ret; }
}

BUTree<? extends SExpr> PrimarySuffix(BUTree<? extends SExpr> scope):
{
	BUTree<? extends SExpr> ret;
}
{
  // Run is started before scope

  (
    LOOKAHEAD(2)
    ret = PrimarySuffixWithoutSuper(scope)
  |
    "." "super" { ret = dress(SSuperExpr.make(optionOf(scope))); }
  |
    ret = MethodReferenceSuffix(scope)
  )
  { return ret; }
}

BUTree<? extends SExpr> PrimarySuffixWithoutSuper(BUTree<? extends SExpr> scope):
{
	BUTree<? extends SExpr> ret;
	BUTree<SName> name;
}
{
  // Run is started before scope

  (
    "."
    (
      "this" { ret = dress(SThisExpr.make(optionOf(scope))); }
    |
      ret = AllocationExpression(scope)
    |
      LOOKAHEAD( [ TypeArguments() ] Name() "(" )
      ret = MethodInvocation(scope)
    |
      ret = FieldAccess(scope)
    )
  |
    "[" ret = Expression() "]" { ret = dress(SArrayAccessExpr.make(scope, ret)); }
  )

  { return ret; }
}

BUTree<? extends SExpr> FieldAccess(BUTree<? extends SExpr> scope):
{
	BUTree<SName> name;
}
{
  // Run is started before scope

  name = Name()
  { return dress(SFieldAccessExpr.make(optionOf(scope), name)); }
}

BUTree<? extends SExpr> MethodInvocation(BUTree<? extends SExpr> scope):
{
    BUTree<SNodeList> typeArgs = null;
	BUTree<SName> name;
    BUTree<SNodeList> args = null;
	BUTree<? extends SExpr> ret;
}
{
  // Run is started before scope

  [ typeArgs = TypeArguments() ] name = Name()
  args = Arguments()
  { return dress(SMethodInvocationExpr.make(optionOf(scope), ensureNotNull(typeArgs), name, args)); }
}

BUTree<SNodeList> Arguments():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<? extends SExpr> expr;
}
{
  "("
  [
    LOOKAHEAD(1)
    expr = Expression() { ret = append(ret, expr); }
    ( "," expr = Expression() { ret = append(ret, expr); } )*
  |
    LOOKAHEAD({quotesMode}) ret = NodeListVar()
  ]
  ")"
  { return ret; }
}

BUTree<? extends SExpr> MethodReferenceSuffix(BUTree<? extends SExpr> scope):
{
    BUTree<SNodeList> typeArgs = null;
	BUTree<SName> name;
	BUTree<? extends SExpr> ret;
}
{
  // Run is started before scope

  "::" [typeArgs = TypeArguments() ]
  ( name = Name() | "new" { name = SName.make("new"); } )
  { ret = dress(SMethodReferenceExpr.make(scope, ensureNotNull(typeArgs), name)); }
  { return ret; }
}

BUTree<? extends SExpr> AllocationExpression(BUTree<? extends SExpr> scope):
{
	BUTree<? extends SExpr> ret;
	BUTree<? extends SType> type;
	BUTree<SNodeList> typeArgs = null;
	BUTree<SNodeList> anonymousBody = null;
	BUTree<SNodeList> args;
	BUTree<SNodeList> annotations = null;
}
{
  // Run may be started before scope
  { if (scope == null) run(); }

  "new"
  [ typeArgs = TypeArguments() ]

  { run(); }
  annotations = Annotations()
  (
    type = PrimitiveType(annotations)
    ret = ArrayCreationExpr(type)
  |
	type = QualifiedType(annotations)
    (
	  ret = ArrayCreationExpr(type)
	|
	  args = Arguments() [ LOOKAHEAD( "{" ) anonymousBody = ClassOrInterfaceBody(TypeKind.Class) ]
      { ret = dress(SObjectCreationExpr.make(optionOf(scope), ensureNotNull(typeArgs), (BUTree<SQualifiedType>) type, args, optionOf(anonymousBody))); }
    )
  )
  { return ret; }
}

BUTree<? extends SExpr> ArrayCreationExpr(BUTree<? extends SType> componentType):
{
	BUTree<? extends SExpr> expr;
	BUTree<SNodeList> arrayDimExprs = emptyList();
	BUTree<SNodeList> arrayDims = emptyList();
	BUTree<SNodeList> annotations = null;
	BUTree<SArrayInitializerExpr> initializer;
}
{
  (
    LOOKAHEAD( Annotations() "[" Expression() "]" )
    arrayDimExprs = ArrayDimExprsMandatory()
    arrayDims = ArrayDims()
    { return dress(SArrayCreationExpr.make(componentType, arrayDimExprs, arrayDims, none())); }
  |
    arrayDims = ArrayDimsMandatory()
    initializer = ArrayInitializer()
    { return dress(SArrayCreationExpr.make(componentType, arrayDimExprs, arrayDims, optionOf(initializer))); }
  )
}

BUTree<SNodeList> ArrayDimExprsMandatory():
{
	BUTree<SNodeList> arrayDimExprs = emptyList();
	BUTree<SNodeList> annotations;
	BUTree<? extends SExpr> expr;
}
{
  (
    LOOKAHEAD( Annotations() "[" Expression() "]" )
    { run(); }
    annotations = Annotations() "[" expr = Expression() "]"
    { arrayDimExprs = append(arrayDimExprs, dress(SArrayDimExpr.make(annotations, expr))); }
  )+
  { return arrayDimExprs; }
}

BUTree<SNodeList> ArrayDimsMandatory():
{
	BUTree<SNodeList> arrayDims = emptyList();
	BUTree<SNodeList> annotations;
}
{
  (
    LOOKAHEAD( Annotations() "[" "]" )
    { run(); }
    annotations = Annotations() "[" "]"
    { arrayDims = append(arrayDims, dress(SArrayDim.make(annotations))); }
  )+
  { return arrayDims; }
}

/**********************************************************************************************************************
 * Statements
 **********************************************************************************************************************/

BUTree<? extends SStmt> Statement():
{
	BUTree<? extends SStmt> ret;
}
{
  (
	  LOOKAHEAD(2)
	  ret = LabeledStatement()
	|
	  ret = AssertStatement()
	|
	  ret = Block()
	|
	  ret = EmptyStatement()
	|
	  ret = StatementExpression()
	|
	  ret = SwitchStatement()
	|
	  ret = IfStatement()
	|
	  ret = WhileStatement()
	|
	  ret = DoStatement()
	|
	  ret = ForStatement()
	|
	  ret = BreakStatement()
	|
	  ret = ContinueStatement()
	|
	  ret = ReturnStatement()
	|
	  ret = ThrowStatement()
	|
	  ret = SynchronizedStatement()
	|
	  ret = TryStatement()
  )
  { return ret; }
}

BUTree<SAssertStmt> AssertStatement():
{
	BUTree<? extends SExpr> check;
	BUTree<? extends SExpr> msg = null;
}
{
  { run(); }
  "assert" check = Expression() [ ":" msg = Expression() ] ";"
  { return dress(SAssertStmt.make(check, optionOf(msg)));  }
}

BUTree<SLabeledStmt> LabeledStatement():
{
	BUTree<SName> label;
	BUTree<? extends SStmt> stmt;
}
{
  { run(); }
  label = Name() ":" stmt = Statement()
  { return dress(SLabeledStmt.make(label, stmt)); }
}

BUTree<SBlockStmt> Block():
{
	BUTree<SNodeList> stmts;
}
{
  { run(); }
  "{" stmts = Statements() "}"
  { return dress(SBlockStmt.make(ensureNotNull(stmts))); }
}

/*
 * Classes inside block stametents can only be abstract or final. The semantic must check it.
 */
BUTree<? extends SStmt> BlockStatement():
{
	BUTree<? extends SStmt> ret;
	BUTree<? extends SExpr> expr;
	BUTree<? extends STypeDecl> typeDecl;
	BUTree<SNodeList> modifiers;
}
{
  (
    LOOKAHEAD( ModifiersNoDefault() ("class" | "interface") )
    { run(); }
    { run(); } modifiers = ModifiersNoDefault() typeDecl = ClassOrInterfaceDecl(modifiers)
    { ret = dress(STypeDeclarationStmt.make(typeDecl)); }
  |
    LOOKAHEAD( VariableDeclExpression() )
    { run(); }
    expr = VariableDeclExpression() ";"
    { ret = dress(SExpressionStmt.make(expr)); }
  |
    ret = Statement()
  )
  { return ret; }
}

BUTree<SVariableDeclarationExpr> VariableDeclExpression():
{
	BUTree<SNodeList> modifiers;
	BUTree<SLocalVariableDecl> variableDecl;
}
{
  { run(); }
  { run(); } modifiers = ModifiersNoDefault() variableDecl = VariableDecl(modifiers)
  { return dress(SVariableDeclarationExpr.make(variableDecl)); }
}

BUTree<SEmptyStmt> EmptyStatement():
{}
{
  { run(); }
  ";"
  { return dress(SEmptyStmt.make()); }
}

BUTree<SExpressionStmt> StatementExpression():
{
	BUTree<? extends SExpr> expr;
	AssignOp op;
	BUTree<? extends SExpr> value;
}
{
  { run(); }
  (
    expr = PrefixExpression()
  |
    expr = PrimaryExpression()
    [
      { lateRun(); } "++"
      { expr = dress(SUnaryExpr.make(UnaryOp.PostIncrement, expr));  }
    |
      { lateRun(); } "--"
      { expr = dress(SUnaryExpr.make(UnaryOp.PostDecrement, expr));  }
    |
      { lateRun(); } op = AssignmentOperator() value = Expression()
      { expr = dress(SAssignExpr.make(expr, op, value)); }
    ]
  )
  ";"
  { return dress(SExpressionStmt.make(expr)); }
}

BUTree<SSwitchStmt> SwitchStatement():
{
	BUTree<? extends SExpr> selector;
	BUTree<SSwitchCase> entry;
	BUTree<SNodeList> entries = emptyList();
}
{
  { run(); }
  "switch" "(" selector = Expression() ")" "{"
  ( entry = SwitchEntry() { entries = append(entries, entry); } )*
  "}"
  { return dress(SSwitchStmt.make(selector, entries)); }
}

BUTree<SSwitchCase> SwitchEntry():
{
	BUTree<? extends SExpr> label = null;
	BUTree<SNodeList> stmts;
}
{
  { run(); }
  (
    "case" label = Expression()
  |
    "default"
  )
  ":" stmts = Statements()
  { return dress(SSwitchCase.make(optionOf(label), ensureNotNull(stmts))); }
}

BUTree<SIfStmt> IfStatement():
/*
 * The disambiguating algorithm of JavaCC automatically binds dangling
 * else's to the innermost if Stmt.  The LOOKAHEAD specification
 * is to tell JavaCC that we know what we are doing.
 */
{
	BUTree<? extends SExpr> condition;
	BUTree<? extends SStmt> thenStmt;
	BUTree<? extends SStmt> elseStmt = null;
}
{
  { run(); }
  "if" "(" condition = Expression() ")" thenStmt = Statement() [ LOOKAHEAD(1) "else" elseStmt = Statement() ]
  { return dress(SIfStmt.make(condition, thenStmt, optionOf(elseStmt))); }
}

BUTree<SWhileStmt> WhileStatement():
{
	BUTree<? extends SExpr> condition;
	BUTree<? extends SStmt> body;
}
{
  { run(); }
  "while" "(" condition = Expression() ")" body = Statement()
  { return dress(SWhileStmt.make(condition, body)); }
}

BUTree<SDoStmt> DoStatement():
{
	BUTree<? extends SExpr> condition;
	BUTree<? extends SStmt> body;
}
{
  { run(); }
  "do" body = Statement() "while" "(" condition = Expression() ")" ";"
  { return dress(SDoStmt.make(body, condition)); }
}

BUTree<? extends SStmt> ForStatement():
{
	BUTree<SVariableDeclarationExpr> varExpr = null;
	BUTree<? extends SExpr> expr = null;
	BUTree<SNodeList> init = null;
	BUTree<SNodeList> update = null;
	BUTree<? extends SStmt> body;
}
{
  { run(); }
  "for" "("
  (
    LOOKAHEAD(VariableDeclExpression() ":")
    varExpr = VariableDeclExpression() ":" expr = Expression()
  |
    [ init = ForInit() ] ";" [ expr = Expression() ] ";" [ update = ForUpdate() ]
  )
  ")" body = Statement()
  {
  	if (varExpr != null) return dress(SForeachStmt.make(varExpr, expr, body));
    else return dress(SForStmt.make(init, expr, update, body));
  }
}

BUTree<SNodeList> ForInit():
{
	BUTree<SNodeList> ret;
	BUTree<? extends SExpr> expr;
}
{
  (
    LOOKAHEAD( Modifiers() Type() Name() )
    expr = VariableDeclExpression() { ret = emptyList(); ret = append(ret, expr); }
  |
    ret = ExpressionList()
  )
  { return ret; }
}

BUTree<SNodeList> ExpressionList():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<? extends SExpr> expr;
}
{
  expr = Expression() { ret = append(ret, expr); } ( "," expr = Expression()  { ret = append(ret, expr); } )*
  { return ret; }
}

BUTree<SNodeList> ForUpdate():
{
	BUTree<SNodeList> ret;
}
{
  ret = ExpressionList()
  { return ret; }
}

BUTree<SBreakStmt> BreakStatement():
{
	BUTree<SName> id = null;
}
{
  { run(); }
  "break" [ id = Name() ] ";"
  { return dress(SBreakStmt.make(optionOf(id))); }
}

BUTree<SContinueStmt> ContinueStatement():
{
	BUTree<SName> id = null;
}
{
  { run(); }
  "continue" [ id = Name() ] ";"
  { return dress(SContinueStmt.make(optionOf(id))); }
}

BUTree<SReturnStmt> ReturnStatement():
{
	BUTree<? extends SExpr> expr = null;
}
{
  { run(); }
  "return" [ expr = Expression() ] ";"
  { return dress(SReturnStmt.make(optionOf(expr))); }
}

BUTree<SThrowStmt> ThrowStatement():
{
	BUTree<? extends SExpr> expr;
}
{
  { run(); }
  "throw" expr = Expression() ";"
  { return dress(SThrowStmt.make(expr)); }
}

BUTree<SSynchronizedStmt> SynchronizedStatement():
{
	BUTree<? extends SExpr> expr;
	BUTree<SBlockStmt> block;
}
{
  { run(); }
  "synchronized" "(" expr = Expression() ")" block = Block()
  { return dress(SSynchronizedStmt.make(expr, block)); }
}

BUTree<STryStmt> TryStatement():
// TODO Make catchClauses/finallyClause counts less fatal
{
	BUTree<SNodeList> resources = null;
	ByRef<Boolean> trailingSemiColon = new ByRef<Boolean>(false);
	BUTree<SBlockStmt> tryBlock;
	BUTree<SBlockStmt> finallyBlock = null;
	BUTree<SNodeList> catchClauses = null;
}
{
  { run(); }
  "try"
  (
    resources = ResourceSpecification(trailingSemiColon)
    tryBlock = Block()
    [ catchClauses = CatchClauses() ]
    [ "finally" finallyBlock = Block() ]
  |
    tryBlock = Block()
    (
      catchClauses = CatchClauses()
      [ "finally" finallyBlock = Block() ]
    |
      "finally" finallyBlock = Block()
    )
  )
  { return dress(STryStmt.make(ensureNotNull(resources), trailingSemiColon.value, tryBlock, ensureNotNull(catchClauses), optionOf(finallyBlock))); }
}

BUTree<SNodeList> CatchClauses():
{
	BUTree<SNodeList> catchClauses = emptyList();
	BUTree<SCatchClause> catchClause;
}
{
  (
    catchClause = CatchClause() { catchClauses = append(catchClauses, catchClause); }
  )+
  { return catchClauses; }
}

BUTree<SCatchClause> CatchClause():
{
	BUTree<SFormalParameter> param;
	BUTree<SBlockStmt> catchBlock;
}
{
  { run(); }
  "catch" "(" param = CatchFormalParameter() ")" catchBlock = Block()
  { return dress(SCatchClause.make(param, catchBlock)); }
}

BUTree<SFormalParameter> CatchFormalParameter():
{
	BUTree<SNodeList> modifiers;
	BUTree<? extends SType> exceptType;
	BUTree<SNodeList> exceptTypes = emptyList();
	BUTree<SVariableDeclaratorId> exceptId;
}
{
  { run(); }
  modifiers = Modifiers()

  exceptType = QualifiedType(null) { exceptTypes = append(exceptTypes, exceptType); }
  [
    LOOKAHEAD("|")
    { lateRun(); }
    ( "|" exceptType = AnnotatedQualifiedType() { exceptTypes = append(exceptTypes, exceptType); } )+
    { exceptType = dress(SUnionType.make(exceptTypes)); }
  ]

  exceptId = VariableDeclaratorId()
  { return dress(SFormalParameter.make(modifiers, exceptType, false, exceptId)); }
}

BUTree<SNodeList> ResourceSpecification(ByRef<Boolean> trailingSemiColon) :
{
	BUTree<SNodeList> vars = emptyList();
	BUTree<SVariableDeclarationExpr> var;
}
{
  "("
  // FIXME this is a bit more lenient than we need to be, eg allowing access modifiers like private
  var = VariableDeclExpression() { vars = append(vars, var); }
  ( LOOKAHEAD(2) ";" var = VariableDeclExpression() {vars = append(vars, var);} )*
  [ LOOKAHEAD(2) ";" { trailingSemiColon.value = true; } ]
  ")"
  { return vars; }
}


/* We use productions to match >>>, >> and > so that we can keep the
 * type Decl syntax with generics clean
 */

void RUNSIGNEDSHIFT():
{}
{
  ( LOOKAHEAD({ getToken(1).kind == GT &&
                getToken(1).realKind == RUNSIGNEDSHIFT } )
   ">" ">" ">" { popNewWhitespaces(); }
  )
}

void RSIGNEDSHIFT():
{}
{
  ( LOOKAHEAD({ getToken(1).kind == GT &&
                getToken(1).realKind == RSIGNEDSHIFT } )
  ">" ">" { popNewWhitespaces(); }
  )
}


/**********************************************************************************************************************
 * Annotations
 **********************************************************************************************************************/

BUTree<SNodeList> Annotations():
{
	BUTree<SNodeList> annotations = emptyList();
	BUTree<? extends SAnnotationExpr> annotation;
}
{
  ( annotation = Annotation() { annotations = append(annotations, annotation); } )*
  { return annotations; }
}

BUTree<? extends SAnnotationExpr> Annotation():
{
	BUTree<? extends SAnnotationExpr> ret;
}
{
  (
    LOOKAHEAD( "@" QualifiedName() "(" ( Name() "=" | ")" ))
    ret = NormalAnnotation()
  |
    LOOKAHEAD( "@" QualifiedName() "(" )
    ret = SingleMemberAnnotation()
  |
    LOOKAHEAD( "@" QualifiedName() )
    ret = MarkerAnnotation()
  )
  { return ret; }
}

BUTree<SNormalAnnotationExpr> NormalAnnotation():
{
	BUTree<SQualifiedName> name;
	BUTree<SNodeList> pairs = null;
}
{
  { run(); }
  "@" name = QualifiedName() "(" [ pairs = MemberValuePairs() ] ")"
  { return dress(SNormalAnnotationExpr.make(name, ensureNotNull(pairs))); }
}

BUTree<SMarkerAnnotationExpr> MarkerAnnotation():
{
	BUTree<SQualifiedName> name;
}
{
  { run(); }
  "@" name = QualifiedName()
  { return dress(SMarkerAnnotationExpr.make(name)); }
}

BUTree<SSingleMemberAnnotationExpr> SingleMemberAnnotation():
{
	BUTree<SQualifiedName> name;
	BUTree<? extends SExpr> memberVal;
}
{
  { run(); }
  "@" name = QualifiedName() "(" memberVal = MemberValue() ")"
  { return dress(SSingleMemberAnnotationExpr.make(name, memberVal)); }
}

BUTree<SNodeList> MemberValuePairs():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<SMemberValuePair> pair;
}
{
  pair = MemberValuePair() { ret = append(ret, pair); }
  ( "," pair = MemberValuePair() { ret = append(ret, pair); } )*
  { return ret; }
}

BUTree<SMemberValuePair> MemberValuePair():
{
	BUTree<SName> name;
	BUTree<? extends SExpr> value;
}
{
  { run(); }
  name = Name() "=" value = MemberValue()
  { return dress(SMemberValuePair.make(name, value)); }
}

BUTree<? extends SExpr> MemberValue():
{
	BUTree<? extends SExpr> ret;
}
{
  (
    ret = Annotation()
  |
    ret = MemberValueArrayInitializer()
  |
    ret = ConditionalExpression()
  )
  { return ret; }
}

BUTree<? extends SExpr> MemberValueArrayInitializer():
{
	BUTree<SNodeList> ret = emptyList();
	BUTree<? extends SExpr> member;
	boolean trailingComma = false;
}
{
  { run(); }
  "{"
  [
    member = MemberValue() { ret = append(ret, member); }
    ( LOOKAHEAD(2) "," member = MemberValue() { ret = append(ret, member); } )*
  ]
  [ "," { trailingComma = true; } ]
  "}"
  { return dress(SArrayInitializerExpr.make(ret, trailingComma)); }
}
