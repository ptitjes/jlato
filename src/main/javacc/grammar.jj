BUTree<SQualifiedType> QualifiedType(BUTree<SNodeList> annotations):
{
    BUTree<SNodeOption> scope = none();
	BUTree<SQualifiedType> ret;
	BUTree<SName> name;
	BUTree<SNodeList> typeArgs = null;
}
{
  // Run may be started before annotations
  {
    if (annotations == null) {
      run();
      annotations = emptyList();
    }
  }

  name = Name()
  [ LOOKAHEAD(2) typeArgs = TypeArgumentsOrDiamond() ]
  { ret = dress(SQualifiedType.make(annotations, scope, name, optionOf(typeArgs))); }
  (
    LOOKAHEAD(2)
    "." { scope = optionOf(ret); }

    { lateRun(); }
    annotations = Annotations() name = Name()
    [ LOOKAHEAD(2) typeArgs = TypeArgumentsOrDiamond() ]
    { ret = dress(SQualifiedType.make(annotations, scope, name, optionOf(typeArgs))); }
  )*

  { return ret; }
